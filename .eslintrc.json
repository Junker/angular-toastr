{
    "env": {
        "browser": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "indent": ["off", "space"],
        "linebreak-style": ["error", "unix"],
        "quotes": ["off", "single"],
        "semi": ["error", "always"],
        "no-unused-vars": "off",
        "no-undef": "off",
        "no-console": "off",
		"no-mixed-spaces-and-tabs": "off"
    }
}
